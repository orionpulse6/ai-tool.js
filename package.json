    "json-canonicalize": "^1.0.6",
    "property-manager": "^2.0.0-alpha.5",
    "secondary-cache": "^2.0.0-alpha.5",
    "secure-json-parse": "^2.7.0",
    "util-ex": "2.0.0-alpha.13",
    "uuid": "^9.0.1",
    "xxhashjs": "^0.2.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.8",
    "@types/xxhashjs": "^0.2.4",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "bumpp": "^9.4.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eventsource": "^2.0.2",
    "fastify": "^4.26.2",
    "onnxruntime-web": "^1.17.3",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "type-fest": "^4.15.0",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "bug": "https://github.com/isdk/ai-tool.js/issues",
  "description": "AI tools",
  "files": [
    "docs",
    "dist"
  ],
  "homepage": "https://github.com/isdk/ai-tool.js/#readme",
  "keywords": [
    "ai",
    "tool"
  ],
  "license": "MIT",
  "maintainers": [
    "Riceball LEE <snowyu.lee@gmail.com>"
  ],
  "engines": {
    "node": ">=20.11.1"
  },
  "packageManager": "pnpm@7.27.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build-fast": "tsup",
    "build": "pnpm run build-fast --dts-resolve --dts && pnpm run doc:md",
    "doc": "typedoc --plugin none --out web",
    "doc:md": "typedoc --plugin typedoc-plugin-markdown --out docs",
    "style": "pnpm run format -c && pnpm run lint",
    "style:fix": "pnpm run format:fix && npm run lint:fix",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,css,scss,sass,less,md}\"",
    "format:fix": "pnpm run format --write",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "test": "vitest run",
    "release": "bumpp package.json --commit --push --tag && pnpm publish --access public",
    "prepublishOnly": "pnpm run build"
  },
  "types": "./dist/index.d.ts"
}
