    ".": "./dist/index.mjs"
  },
  "dependencies": {
    "@huggingface/jinja": "^0.2.2",
    "@xenova/transformers": "^2.16.1",
    "abstract-error": "^2.0.0-alpha.8",
    "base32768": "^3.0.1",
    "custom-ability": "^2.0.0-alpha.6",
    "custom-factory": "2.3.0-alpha.6",
